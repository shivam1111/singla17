<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

    <!-- Sequences for mill.purchase.order -->
    <record id="seq_mill_purchase_order" model="ir.sequence">
        <field name="name">Mill Purchase Order</field>
        <field name="code">mill.purchase.order</field>
        <field name="prefix">SSAI-PO</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_stock_line" model="ir.sequence">
        <field name="name">Stock Line</field>
        <field name="code">stock.line</field>
        <field name="prefix">SL</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_heat_heat" model="ir.sequence">
        <field name="name">Heat</field>
        <field name="code">heat.heat</field>
        <field name="prefix">H</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    </data>
</odoo>
